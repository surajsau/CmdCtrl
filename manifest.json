{
  "manifest_version": 2,

  "name": "Ctrl!",
  "description": "Extension to help you browse Java github code like in IDE",
  "version": "1.0.0",
  "background": {
    "scripts": [
      "jquery-1.11.3.min.js",
      "background.js"
    ]
  },
  "content_scripts": [{
    "matches": ["*://*.github.com/*"],
    "js": [
      "content.js",
      "jquery-1.11.3.min.js"
    ]
  }],
  "browser_action": {
   "default_icon": "icon.png"
  },
  "permissions": [
   "tabs",
   "*://*.github.com/*",
   "background"
   ]
}